  ],
  "license": "CC-BY-NC-ND-4.0",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "dev:android": "expo run:android -d # run on device",
    "build:local": "eas build -p android --profile apk --local",
    "build": "eas build -p android --profile apk --no-wait",
    "deploy": "eas build -p android --no-wait",
    "dev:ios": "expo run:ios -d # run on device",
    "deploy:ios": "eas build -p ios --no-wait",
    "dev:web": "next dev",
    "build:web": "next build",
    "test": "jest --watchAll",
    "lint": "eslint --fix \"{,!(node_modules|android|ios)/**/}*.{ts,tsx}\"",
    "precommit": "yarn run lint",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "jest": {
    "preset": "jest-expo"
  },
